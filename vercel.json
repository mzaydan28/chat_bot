{
    "builds": [
      {
        "src": "*.php",
        "use": "vercel-php@0.7.1"
      },
      {
        "src": "**/*.php",
        "use": "vercel-php@0.7.1"
      },
      {
        "src": "assets/**",
        "use": "@vercel/static"
      },
      {
        "src": "public/**",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/(assets|public)/.*",
        "dest": "/$0"
      },
      {
        "src": "/(.*)",
        "dest": "/index.php"
      }
    ]
  }