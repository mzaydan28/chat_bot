{
    "version": 2,
    "builds": [
      {
        "src": "assets/**",
        "use": "@vercel/static"
      },
      {
        "src": "*.php",
        "use": "vercel-php@0.7.1"
      },
      {
        "src": "public/*.php",
        "use": "vercel-php@0.7.1"
      }
    ],
    "routes": [
      {
        "src": "/assets/(.*)",
        "dest": "/assets/$1"
      },
      {
        "src": "/public/(.*)",
        "dest": "/public/$1"
      },
      {
        "src": "/(.*)",
        "dest": "/index.php"
      }
    ]
  }